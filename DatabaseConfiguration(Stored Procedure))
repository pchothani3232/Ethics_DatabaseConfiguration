--[LoginCredential] @status='test'
ALTER    procedure [dbo].[LoginCredential]
 @Username nvarchar(50) = '',
 @Password nvarchar(50) = '',
 @Status nvarchar(50) = ''
As
Begin

	if @status='Select'
	Begin
		select * from Users where Username=@Username AND Password=@Password;
	end

	else if @status='test'
	Begin
		select 1 AS test
	end
end	
